import math


def expression (x):

    z = (1 - 2 * math.sin(x)**2) / (1 + math.sin(x)**2)

    return z

def resalt (x, y):

    sum = 0

    for i in range (x, y + 1):

        if i % 2 == 1:

            sum += i

    return sum

x = int(input("Введіть значення x: "))

print ("Значення виразу z = ", expression(x))

x = int(input("Введіть від якого числа (х) знаходити суму усіх непарних чисел: "))

y = int(input("Введіть до якого числа (y) знаходити суму усіх непарних чисел: "))

while x > y:

    x = int(input("у не може бути менше х, введіть ще раз від якого числа (х) знаходити суму усіх непарних чисел: "))

    y = int(input("Введіть до якого числа (y) знаходити суму усіх непарних чисел: "))

print("Сума непарних чисел від х до у = ", resalt (x, y))
